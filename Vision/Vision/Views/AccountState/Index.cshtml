
@{
    ViewData["Title"] = "Index";
}

<table id="example" class="display" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Position</th>
            <th>Office</th>
            <th>Extn.</th>
            <th>Start date</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <th>Name</th>
            <th>Position</th>
            <th>Office</th>
            <th>Extn.</th>
            <th>Start date</th>
            <th>Salary</th>
        </tr>
    </tfoot>
</table>

<script>
    $(document).ready(function () {
        $.ajax({
            url: 'http://localhost:54214/AccountState/GetAll',
            method: "GET",
            xhrFields: {
                withCredentials: true
            },
            success: function (rs) {
                debugger;
                var ds = rs.data;
                $('#example').DataTable({
                    "aaData": ds,
                    "columns": [
                        { "data": "id" },
                        { "data": "symbol" },
                        { "data": "description" },
                        { "data": "note" },
                        { "data": "type" },
                        { "data": "department" }
                    ]
                });
            }
        });
    });
</script>